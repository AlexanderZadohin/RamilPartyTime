<Page x:Class="RamilPartyTime.View.Pages.HiFriendScreen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RamilPartyTime.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="670" d:DesignWidth="540"
      Title="HiFriendScreen">

    <Grid>
        <Button BorderThickness="0" Background="{x:Null}" Click="RegistrationBtn_Click" Name="RegistrationBtn" Margin="0,410,0,0"  VerticalAlignment="Top" HorizontalAlignment="Center" Width="438" Height="136">
            <Image Source="/Resources/Images/SignButton.png" />
        </Button>
        <Button BorderThickness="0" Background="{x:Null}"  Margin="0 0 0 30" Click="EnterBtn_Click" Name="EnterBtn" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="400">
            <Image Source="/Resources/Images/SignupButton.png" />
        </Button>
    </Grid>
</Page>
